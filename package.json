{
  "name": "rush-the-line",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "npx expo start -c --dev-client",
    "ios:dev:build": "eas build --platform ios --profile development",
    "android:dev:build": "eas build --platform android --profile development",
    "ios:preview:build": "eas build --platform ios --profile preview",
    "ios:preview:update": "eas update --branch preview --platform ios",
    "android:preview:build": "eas build --platform android --profile preview",
    "android:preview:update": "eas update --branch preview --platform android",
    "prebuild": "npm install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.5.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.4.0",
    "@supabase/supabase-js": "^2.76.1",
    "expo": "54.0.23",
    "expo-av": "~16.0.7",
    "expo-dev-client": "~6.0.17",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-location": "^19.0.7",
    "expo-status-bar": "~3.0.8",
    "lucide-react-native": "^0.546.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "typescript": "~5.9.2"
  },
  "private": true
}
